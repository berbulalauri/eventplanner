
@model IEnumerable<practice_02.Models.AccountInfo>

<header>
    @{
        await Html.RenderPartialAsync("../PartialViews/_successfulRegister.cshtml");
    }
</header>

<div class="container">
<h1>
    Account: @Model.Last().UserName was Created with 
</h1>
<h1>
    id: @Model.Last().ClientId
</h1>

</div>


<div class="submitButtonCentered" style="display: inline-block; position: relative; left: 17%; top: 10px;">
    <a class="btn btn-success" role="button" href="@Url.Action("AddAccount")" style="color:inherit">Add New Account</a>
    <a class="btn btn-secondary" role="button" href="~/Index" style="color:inherit">Back</a>
</div>
